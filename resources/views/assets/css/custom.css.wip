    /* border: 3px solid #f7f7f7; */
    box-shadow: inset 0px 0px 0px 4px #f7f7f7;
    box-sizing: border-box;
}

.sv-portlet--bordered-red {
    box-shadow: inset 0px 0px 0px 4px #ffeaed;
}

/** 
** Bootstrap Extensions
**/

.btn-fix-height {
    height: 46px;
}

.sub-dropdown-link:hover .sub-dropdown {
    display: block;
}

.dropdown-menu.sub-dropdown {
    left: 99%;
    right: auto;
    top: 0;
}

.dropdown--dark .kt-nav__item:hover > .kt-nav__link > .kt-nav__link-icon,
.dropdown--dark .kt-nav__item:hover > .kt-nav__link > .kt-nav__link-text {
    color: #FFF !important;
}

.dropdown--dark .kt-nav__item > .kt-nav__link {
    width: 95%;
    margin: 0 auto;
    border-radius: 5px;
    overflow: hidden;
}
.dropdown--dark .kt-nav__item:hover > .kt-nav__link {
    background: #4e5d7b !important;
    color: #FFF !important;
}

.dropdown-menu.dropdown--dark-full,
.dropdown-menu.dropdown--dark-full > .dropdown-item  {
    background: #2f4368;
    color: #FFF;
    padding: 0.5rem 1.2rem 0.5rem;
    border-radius: 10px;
}

.dropleft .dropdown-menu.dropdown--dark-full {
    margin-right: -15px;
}

.dropdown-item i {
    min-width: 25px;
}

.dropdown-popover .dropdown-toggle:before {
    content: none;
}
.dropleft-reset-arrow [data-toggle="dropdown"]:before {
    content: none !important;
}

/**
** Popovers
**/

.popover {
    background: #2f4368;
    border-radius: 10px;
    border: none;
    min-width: 150px;
}

.popover-trigger:focus i,
.popover-trigger:active i {
    color: #2AA4F2;
}

.popover.bs-popover-left .arrow:after {
    border-left-color: #2f4368 !important;
}

.popover.bs-popover-right .arrow:after {
    border-right-color: #2f4368 !important;
}

.popover.bs-popover-top .arrow:after {
    border-top-color: #2f4368 !important;
}

.popover.bs-popover-bottom .arrow:after {
    border-bottom-color: #2f4368 !important;
}

.popover .popover-link {
    color: #FFF;
    display: block;
    padding: 5px;
}

.popover .popover-link i {
    width: 25px !important;
    display: inline-block;
}
.popover {
    color: #FFF;
}

.popover {
    background: #2D416A;
}


/**
**   Alerts
**/

.alert-outline-blue {
    border: 1px solid #38a5ed;
    color: #38a5ed;
    
}

.alert-semi-filled-blue {
    background: rgba(56, 165, 237, 0.05);
}
.alert.alert-sm {
    padding: 5px;
    font-size: 10px;
}

.alert.alert-round {
    border-radius: 200px;
}

/** 
**  Table Popover
**/

.tbl-popover-container {
    position: relative;
}

.tbl-popover {
    background: #2f4368;
    border-radius: 10px;
    padding: 10px;
    color: #FFF;
    white-space: normal;
    width: 200px;
    left: -50%;
    font-size: 12px;
    position: absolute;
}

/**
**  KT NAV ICON
**/

.dropdown--dark .kt-nav__item:hover > .kt-nav__link > .kt-nav__link-icon-hover-green {
    color: #7EFF00 !important;
}


