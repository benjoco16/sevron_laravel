    content: '\f11f';
    font-size: 1.5rem;
}
@media screen and (max-width: 1023px) {
    .risk-assessment__title,
    .risk-assessment__controls {
        margin-bottom: 30px;
    }

    .risk-assessment__title > .d-flex {
        flex-direction: column;
    }
}



/** DRAGGABLE STATUS STYLE **/

.accordion[data-status="category"] .accordion-status {
    color: #8C26E6;
    border-color: #8C26E6;
    background-color: #F4E6FF;
}

.accordion.accordion-light[data-status="category"] .risk-assessment {
    border-color: #8C26E6;
}

.accordion .accordion .risk-assessment {
    border-top: 1px solid #cfcfcf !important;
    border-bottom: 1px solid #cfcfcf !important;
    border-right: 1px solid #cfcfcf !important;
}

/** NOT STARTED STATUS STYLE **/

.accordion.accordion-light[data-status="not-started"] .risk-assessment {
