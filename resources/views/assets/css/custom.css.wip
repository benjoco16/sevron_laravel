.ban_text {
    max-width: 85%;
    margin: 52px auto;
}
.ban_text p {
    color: #44a4ee;
    margin-bottom: 0;
    font-weight: 500;
    font-size: 11px;
}
.ban_text .score_hldr { 
    color: #2e4269;
    background-color: #fce04a;
    width: 73px;
    text-align: center;
    border-radius: 17px;
    font-size: 19px;
    font-weight: 600;
    padding: 3px 0 3px 0;
    line-height: auto;
    margin-left: 3px;
}
.ban_text h2 {
    color: #2e4269;
    font-weight:600;
    font-size: 24px;
    font-family: 'Poppins';
    line-height: 39px;
}
.ban_text .top_list {
    margin-top: 20px;
    font-size: 14px;
    line-height: 20px;
    color: #2e4269;
}
.ban_text .top_list ul {
    line-height: 1.7;
    margin: 13px 0 26px;
    font-size: 12px;
}

.ban_text .top_list ul .kt-badge {
    margin-right: 10px;
}
.ban_text .top_list h4 {
    font-weight: 600;
    font-size: 16px;
    margin-bottom: 11px;
    font-family: 'Poppins'
}
.ban_text .top_list ul li strong {
    font-weight: 600;
}
.ban_text .ban_bt {
    margin-top: 20px;
    color: #5570a3;
    font-weight: normal;
    font-size: 12px;
    line-height: 15px;
}
.ban_text .ban_bt strong {
    font-weight: 600;
}     
.ban_text .ban_bt button {
    margin-right: 10px;
    font-weight: 500;
    font-size: 13px;
    border: none;
    -webkit-box-shadow: 0px 4px 18px -13px rgba(0,0,0,0.75);
    -moz-box-shadow: 0px 4px 18px -13px rgba(0,0,0,0.75);
    box-shadow: 0px 4px 18px -13px rgba(0,0,0,0.75);
}
.ban_text .ban_bt span {
    display: inline-block;
    vertical-align: middle;
    line-height: 1.5;
}

