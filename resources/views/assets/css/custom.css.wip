}
.nopadding {
    padding: 0;
}

.margin_right {
    margin-right: 10px;
}
.accordion.accordion-light .risk-assessment {
    margin-bottom: 10px;
    border-radius: 12px;
    -webkit-box-shadow: 1px 0px 14px -11px rgba(0,0,0,0.75);
    -moz-box-shadow: 1px 0px 14px -11px rgba(0,0,0,0.75);
    box-shadow: 1px 0px 14px -11px rgba(0,0,0,0.75);
    border-left: 8px solid #30a4f0;
}


.accordion.accordion-light .risk-assessment .card-header {
    padding: 0 15px;
}

.risk-assessment__portlet--bordered {
    border: 1px solid map-get($custom-colors, 'silver') !important;
}

.risk-assessment .accordion-draggable {
    margin-right: 10px;
}

.risk-assessment .accordion-draggable i {
    color: #989898;
    margin-right: -3px;
}

.accordion .accordion-status {
    border-radius: 17px !important;
    margin-right: 15px;
    font-size: 12px;
    text-transform: uppercase;
    padding: 3px 15px;
    border: 1px solid #30a4f0;
}

