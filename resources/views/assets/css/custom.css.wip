/** CALENDAR **/
.fc-ltr .fc-dayGrid-view .fc-day-top .fc-day-number {
    float: none;
}
#new_cald .fc-widget-content, 
#new_cald .fc-widget-header {
    border: none !important;
}
tr:first-child > td > .fc-day-grid-event {
    box-shadow: unset;
}
.fc-unthemed .fc-day-grid td:not(.fc-axis) {
    text-align: center;
}
.fc-unthemed .fc-event.fc-start .fc-content:before, .fc-unthemed .fc-event-dot.fc-start .fc-content:before {
    background: #e2e5ec;
    display: block;
    text-align: center;
    margin: 0 auto;
    position: relative;
    top: 0;
    left: 0;
}
.raphael-group-115-item > path[stroke-width="2"] {
    display: none !important;
}
.fc-unthemed .fc-event .fc-content {
    text-align: center;
    padding: 0;
    top: 3px;
}
.fc-unthemed .fc-event .fc-title,
.raphael-group-192-creditgroup,
.raphael-group-311-creditgroup {
    visibility: hidden;
    display: none;
}
.fc-unthemed .fc-toolbar .fc-button {
    border: none;
}

.fc-unthemed .fc-day-grid td:not(.fc-axis) {
    padding-top: 0;
    padding-bottom: 0;
}

tr:first-child > td > .fc-day-grid-event {
    margin-top: -4px;
}

.fc-content:before {
    background: #30a4f0 !important;
}

.fc-today {
    position: relative;
}

.fc-today span {
    position: relative;
    z-index: 2;
}

.fc-today:after {
    content: '';
    width: 43px;
    height: 43px;
    background: rgba(48, 164, 240, 0.1);
    display: block;
    position: absolute;
    left: 0;
    right: 0;
    margin: auto;
    top: -10px;
    border-radius: 100px;
}

.fc-unthemed .fc-toolbar h2 {
    font-size: 12px;
    font-weight: 700;
    margin-top: 0.75rem;
    text-transform: none;
    color: #707070;
}

.fc-day-header span {
    font-size: 11px;
}


.rsum h2 {
    font-size: 2.8rem;
    font-weight: 600;
